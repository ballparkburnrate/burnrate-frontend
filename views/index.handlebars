  <!-- START MAIN -->
  <div id="main">
    <!-- START WRAPPER -->
    <div class="wrapper">
      <!-- START LEFT SIDEBAR NAV-->
      <aside id="left-sidebar-nav">
        <ul id="slide-out" class="side-nav fixed leftside-navigation">
          <li>
            <a class="waves-effect waves-dark btn modal-trigger" href="#signin">
              <i class="material-icons">portrait</i>
              <span class="waves-effect waves-block waves-light nav-text">Sign In</span>
            </a>
            <li>
                <a class="waves-effect waves-dark btn modal-trigger" href="#signup">
                    <i class="material-icons">portrait</i>
                    <span class="waves-effect waves-block waves-light nav-text">Sign Up</span>
                  </a>
            </li>
          </li>
          <!-- Dashboard Button doesn't add any functionality -->
          <!-- <li>
            <a class="waves-effect waves-dark btn" href="" target="_blank">
